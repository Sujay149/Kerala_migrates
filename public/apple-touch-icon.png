<svg width="180" height="180" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="appleTouchGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0E7490;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#0D9488;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0F766E;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main Circle Background -->
  <circle cx="90" cy="90" r="80" fill="url(#appleTouchGradient)" stroke="#FFFFFF" stroke-width="4"/>
  
  <!-- Central Medical Cross/Plus -->
  <g transform="translate(90,90)">
    <!-- Cross Background Circle -->
    <circle cx="0" cy="0" r="35" fill="white" fill-opacity="0.95"/>
    
    <!-- Medical Cross -->
    <rect x="-4" y="-18" width="8" height="36" fill="url(#appleTouchGradient)" rx="2"/>
    <rect x="-18" y="-4" width="36" height="8" fill="url(#appleTouchGradient)" rx="2"/>
    
    <!-- Small Heart in Center -->
    <path d="M-6 -2 C-6 -5 -3 -7 0 -5 C3 -7 6 -5 6 -2 C6 1 0 6 0 6 C0 6 -6 1 -6 -2" 
          fill="#FF6B6B" opacity="0.8"/>
  </g>
  
  <!-- MediBot Text (Bottom) -->
  <text x="90" y="150" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle" opacity="0.95">MediBot</text>
</svg>