<svg width="512" height="512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="maskableGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0E7490;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#0D9488;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0F766E;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Safe Area Background (for maskable icons) -->
  <rect width="512" height="512" fill="url(#maskableGradient)"/>
  
  <!-- Central Content Area (within safe zone) -->
  <g transform="translate(256,256)">
    <!-- Main Medical Cross Background -->
    <circle cx="0" cy="0" r="120" fill="white" fill-opacity="0.95"/>
    
    <!-- Large Medical Cross -->
    <rect x="-12" y="-60" width="24" height="120" fill="url(#maskableGradient)" rx="4"/>
    <rect x="-60" y="-12" width="120" height="24" fill="url(#maskableGradient)" rx="4"/>
    
    <!-- Central Heart -->
    <path d="M-20 -8 C-20 -16 -10 -24 0 -16 C10 -24 20 -16 20 -8 C20 0 0 20 0 20 C0 20 -20 0 -20 -8" 
          fill="#FF6B6B" opacity="0.9"/>
    
    <!-- Four Corner Health Icons -->
    <!-- Top Left - Stethoscope -->
    <g transform="translate(-80,-80)">
      <circle cx="0" cy="0" r="8" fill="white" opacity="0.8"/>
      <path d="M-5 0 Q-5 -8 0 -8 Q5 -8 5 0" stroke="white" stroke-width="2" fill="none"/>
      <path d="M-5 0 Q-12 8 -12 18" stroke="white" stroke-width="2" fill="none"/>
      <path d="M5 0 Q12 8 12 18" stroke="white" stroke-width="2" fill="none"/>
    </g>
    
    <!-- Top Right - Pills -->
    <g transform="translate(80,-80)">
      <ellipse cx="0" cy="0" rx="10" ry="18" fill="white" opacity="0.8"/>
      <rect x="-10" y="-4" width="20" height="8" fill="url(#maskableGradient)" opacity="0.6"/>
    </g>
    
    <!-- Bottom Left - Shield -->
    <g transform="translate(-80,80)">
      <path d="M-8 -12 Q-8 -16 0 -16 Q8 -16 8 -12 L8 8 Q0 16 -8 8 Z" 
            fill="white" opacity="0.8"/>
    </g>
    
    <!-- Bottom Right - Heart Rate -->
    <g transform="translate(80,80)">
      <path d="M-15 0 L-8 -8 L0 8 L8 -12 L15 0" 
            stroke="white" stroke-width="3" fill="none"/>
    </g>
  </g>
  
  <!-- Kerala Text (Top) -->
  <text x="256" y="80" font-family="Arial, sans-serif" font-size="36" font-weight="bold" fill="white" text-anchor="middle">KERALA</text>
  
  <!-- MediBot Text (Bottom) -->
  <text x="256" y="450" font-family="Arial, sans-serif" font-size="40" font-weight="bold" fill="white" text-anchor="middle">MediBot</text>
  
  <!-- Digital Health Subtitle -->
  <text x="256" y="420" font-family="Arial, sans-serif" font-size="24" fill="white" text-anchor="middle" opacity="0.9">DIGITAL HEALTH</text>
</svg>